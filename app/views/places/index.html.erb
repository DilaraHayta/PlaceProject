<% @places.each do |place| %>
    <b>ID:</b><%= place.id %>
    <b>Name: </b><%= link_to place.name, place_path(place) %>
    <b>Address: </b><%= place.adress %>
    <b>Phone Number: </b><%= place.phone_number %>
    <b>Contact Mail: </b><%= place.contact_mail %>
    <b>Established: </b><%= place.established_at %>
    <b>Description: </b><%= place.description %>
    <b>Category: </b><%= place.category.name unless place.category.nil? %>


    <br><br>

<% end %>
